{
    "name": "phpjit/apidoc-tests-generator",
    "description": "Generate PHPUnit test classes for a apidoc.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Souza Taciclei",
            "email": "souza@taciclei.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "nikic/php-parser": "^4.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^5.0",
        "phpunit/phpunit": "^7.4",
        "fakerphp/faker": "^1.14",
        "hautelook/alice-bundle": "^2.9",
        "jetbrains/phpstorm-attributes": "^1.0",
        "justinrainbow/json-schema": "^5.2",
        "nesbot/carbon": "^2.49"
    },
    "autoload": {
        "psr-4": {
            "PhpJit\\ApidocTestsGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpJit\\ApidocTestsGenerator\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "symfony": {
            "require": "^3.4 || ^4.4 || ^5.1"
        }
    }
}
